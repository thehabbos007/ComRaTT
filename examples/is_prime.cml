mod_op : int -> int -> int
def mod_op n m =
  n - ((n / m) * m);

check_prime_helper : int -> int -> bool
def check_prime_helper n divisor =
  if (divisor * divisor > n)
  then true
  else
    (if ((mod_op n divisor) = 0)
     then false
     else (check_prime_helper n (divisor + 1)));

is_prime : int -> bool
def is_prime n =
  if (n <= 1)
  then false
  else (check_prime_helper n 2);

main : int -> bool
def main x = is_prime x;
